{
  "presets": [
    "env",
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    "styled-jsx/babel",
    "babel-plugin-transform-decorators-legacy",
    "babel-plugin-transform-decorators",
    ["react-intl", {
      "messagesDir": "./translations/messages/"
    }]
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    }
  }
}
